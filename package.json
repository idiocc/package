{
  "name": "my-new-package",
  "version": "0.0.0-pre",
  "description": "{{ description }}",
  "main": "compile/index.js",
  "module": "src/index.js",
  "scripts": {
    "t": "zoroaster -a",
    "test": "yarn t test/spec test/mask",
    "spec": "yarn t test/spec",
    "mask": "yarn t test/mask",
    "compile": "depack src/bin/mnp -o compile/bin/mnp.js -a -c -S --externs types/externs.js",
    "lib": "depack src/depack.js -o compile/mnp.js -a -c --source_map_include_content",
    "test-build": "ALAMODE_ENV=test-build yarn test",
    "test-compile": "ALAMODE_ENV=test-compile yarn test",
    "lint": "eslint .",
    "doc": "NODE_DEBUG=doc doc -o README.md -n _myNewPackage",
    "wiki": "NODE_DEBUG=doc doc documentary-wiki -W wiki -n _myNewPackage",
    "b": "alamode src -o build -s",
    "d": "yarn-s template d1 externs",
    "d1": "typal compile/index.js -u -t types/index.xml",
    "template": "typal compile/template.js -T compile/index.js -t types/api.xml",
    "externs": "typal types/externs.js",
    "args": "argufy -o src/bin/get-args.js"
  },
  "bin": {
    "mnp": "compile/bin/myNewPackage.js",
    "mnp-dev": "src/bin/index.js"
  },
  "files": [
    "build",
    "src",
    "compile",
    "types/externs.js",
    "src/bin/index.js"
  ],
  "externs": "types/externs.js",
  "repository": {
    "type": "git",
    "url": "{{ git_url }}"
  },
  "keywords": [
    "{{ keywords }}"
  ],
  "author": "{{ author_name }} <{{ author_email }}>",
  "license": "MIT",
  "bugs": {
    "url": "{{ issues_url }}"
  },
  "homepage": "{{ readme_url }}",
  "devDependencies": {
    "alamode": "^3.2.0",
    "argufy": "^1.7.1",
    "depack": "^1.0.0",
    "documentary": "^1.34.1",
    "erte": "^1.2.0",
    "eslint-config-artdeco": "1.0.1",
    "indicatrix": "^1.4.0",
    "usually": "^1.2.0",
    "yarn-s": "1.1.0",
    "zoroaster": "^4.1.2"
  }
}
